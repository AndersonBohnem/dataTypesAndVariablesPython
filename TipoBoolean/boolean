import os

def clearConsole():
    os.system('cls')

#Tipo Boolean

clearConsole()
oneVariable = True
twoVariable = False

print("Vari치vel 1: " , oneVariable)
print("Vari치vel 2: " , twoVariable)

print("\nOperadores L칩gicos:")

operatorAnd = oneVariable and twoVariable
print("\nUtilizando o Operador And\nCompara:" , oneVariable , "com" , twoVariable , "que resulta em:" , operatorAnd)

operatorOr = oneVariable or twoVariable
print("\nUtilizando o Operador Or\nCompara:" , oneVariable , "com" , twoVariable , "que resulta em:" , operatorOr)

operatorNot = not oneVariable
print("\nUtilizando o Operador Not\nNessa vari치vel:" , oneVariable , "que resulta em:" , operatorOr)